package javaSandboxOneLevelOne;

public class MethodsClass 
	{

		static void Name(String $Name)
			{
				System.out.println("Hello "+$Name);
			}
		static int Return(String $Name, int x, int y) 
			{
				Name($Name);
				int $x = x;
				int $y = y;
			 	System.out.print("How is your day going today?"+x+y);
				return x+y;
			}
		
	public static void main(String [] args)
		{
			Name("John");
			Name("Tom");
			Name("Bill");
			
		}
	}