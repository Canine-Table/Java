package javaSandboxOneLevelOne;

public class OverLoadedMethods {
	String $Name = null;
	String $Names = null;
	OverLoadedMethods(String _Name)
		{
			this.$Name = _Name;
		}
	void Status(String _Names)
		{
			this.$Names = _Names;
		}
	public static void main(String [] args)
		{
		OverLoadedMethods John = new OverLoadedMethods("John");
		John.Status("CEO");
		System.out.println(John);
		}
}
